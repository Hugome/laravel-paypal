{
    "name": "sger/paypal",
    "description": "A Paypal bridge for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "paypal"],
    "authors": [
        {
            "name": "Spiros Gerokostas",
            "email": "spiros.gerokostas@gmail.com"
        }
    ],
    "require": {
    	"php": "^5.5.9 || ^7.0",
        "illuminate/contracts": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*",
        "graham-campbell/manager": "^2.3",
        "paypal/rest-api-sdk-php": "1.7.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Sger\\Paypal\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
